#include <version.h>

namespace jcbm {
	const uint64_t Version::major = 0;
	const uint64_t Version::minor = 0;
	const uint64_t Version::patch = 0;
	const char* Version::hashcode = "HASH";
	const char* Version::updated = "UPDATED";
	const std::string Version::full = std::to_string(major) + "." + std::to_string(minor) + "." + std::to_string(patch) + " (updated: UPDATED, hash: HASH)";
}
